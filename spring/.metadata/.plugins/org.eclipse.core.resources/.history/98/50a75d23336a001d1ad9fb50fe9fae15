<mapper namespace="com.loven.jy.mapper">

  <select id="getLists" resultType="Boardjy">
     select * from boardjy order by idx desc
  </select>
  <insert id="boardInsert" parameterType="Boardjy">
  	insert into boardjy(title,writer,content)
  	values(#{title},#{writer},#{content})
  </insert>

    
